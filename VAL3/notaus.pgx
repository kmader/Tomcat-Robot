    <source>
      <code>begin
  sioLink(sNetwork,io:tomcatrobot)
  while true
    if sSocket==&quot;stop&quot;
      //sSockets=&quot;stop rcve&quot;
      call sendSocket()
      stopMove()
      taskKill(&quot;MainLoop&quot;)
      disablePower()
      sSockets=&quot;stop success&quot;
      call sendSocket()
      putln(&quot;robot stopped&quot;)
    endIf
  endWhile
end

      </code>
